{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "name": "items",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "total_available": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "apparel",
        "artwork",
        "album"
      ]
    },
    "price": {
      "type": "decimal",
      "required": true
    }
  }
}
