    },
    "time": {
      "type": "time",
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "artists": {
      "via": "events",
      "collection": "artist"
    },
    "ticket_store": {
      "via": "event",
      "model": "ticket-store"
    },
    "merch_store": {
      "via": "event",
      "model": "merch-store"
    }
  }
}
